<!-- block begin -->
<div class="ttl">
    <span class="text">
        HTTP Console
    </span>
</div>

<div class="listcon">
<form id="http-console-dummy-form" method="POST"></form>
<form onsubmit="doHttpRequest(this);return false;">
<ul>
<li>
    <select id="http-console-meth">
    [%- FOREACH meth IN ['GET', 'POST', 'PUT', 'DELETE'] %]
        <option value ="[% meth %]">[% meth %]</option>
    [%- END %]
    </select>
    &nbsp;
    /=/<input id="http-console-url" type="text" style="width: 40em;"></input>
</li>
<li>
    <label for="http-console-body">Content</label><br/>
    <textarea id="http-console-body" rows="5" cols="50"></textarea>
</li>
<li>
    <input type="submit" value="Go!" style="width: 5em"></input>
</li>
<li>
    <label for="http-console-out">Response</label><br/>
    <pre id="http-console-out"></pre>
</li>
</ul>
</form>
</div>

<div class="ttl">
    <span class="text">
        RunView Console
    </span>
</div>

<div class="listcon">
<ul>
<li>
    <textarea rows="8" cols="60"></textarea>
</li>
</ul>
</div>

<div class="ttl">
    <span class="text">
        RunAction Console
    </span>
</div>

<div class="listcon">
<ul>
<li>
    <textarea rows="8" cols="60"></textarea>
</li>
</ul>
</div>

